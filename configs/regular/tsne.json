{
    "PARAMETERS_SETS": {
        "sklearn implementations": {
            "algorithm": [
                { "library": "sklearn", "device": "cpu" },
                { "library": "sklearnex", "device": ["cpu", "gpu"] }
            ]
        },
        "common tsne parameters": {
            "algorithm": {
                "estimator": "TSNE",
                "estimator_params": {
                    "n_components": 2,
                    "perplexity": 30.0,
                    "early_exaggeration": 12.0,
                    "learning_rate": 200.0,
                    "n_iter": 1000,
                    "n_iter_without_progress": 300,
                    "min_grad_norm": 1e-7,
                    "metric": "euclidean",
                    "init": "random",
                    "random_state": 42,
                    "method": "barnes_hut",
                    "angle": 0.5
                }
            }
        },
        "datasets": [
            {
                "data": {
                    "dataset": "hepmass",
                    "split_kwargs": { "train_size": [1000, 2000, 5000] }
                }
            },
            {
                "data": {
                    "dataset": ["a9a", "mnist", "gisette"],
                    "split_kwargs": { "train_size": 5000 }
                }
            },
            {
                "data": {
                    "dataset": "california_housing",
                    "split_kwargs": { "train_size": 10000 }
                }
            }
        ]
    },
    "TEMPLATES": {
		"sklearn tsne": {
			"SETS": ["sklearn implementations", "common tsne parameters", "datasets"],
            "algorithm": {
                "n_jobs": "[SPECIAL_VALUE]physical_cpus"
            }
		},
        "cuml tsne": {
			"SETS": ["common tsne parameters", "datasets"],
            "algorithm": {
                "library": "cuml",
                "estimator_params": { "learning_rate_method": "none", "n_neighbors": 91 }
            },
            "data": { "format": "cudf" }
		}
	}
}
