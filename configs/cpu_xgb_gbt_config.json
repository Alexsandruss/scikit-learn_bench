{
    "omp_env": ["OMP_NUM_THREADS", "OMP_PLACES"],
    "common": {
        "lib": ["modelbuilders"],
        "data-format": ["pandas"],
        "data-order": ["F"],
        "dtype": ["float32"],
        "count-dmatrix": [""]
    },
    "cases": [
        {
            "algorithm": "xgb_mb",
            "dataset": [
                {
                    "source": "csv",
                    "name": "higgs1m",
                    "training":
                    {
                        "x": "../sklbench_data/higgs1m_x_train.csv",
                        "y": "../sklbench_data/higgs1m_y_train.csv"
                    }
                }
            ],
            "reg-alpha": [0.9],
            "max-bin": [256],
            "scale-pos-weight": [2],
            "learning-rate": [0.1],
            "subsample": [1],
            "reg-lambda":  [1],
            "min-child-weight": [0],
            "max-depth": [8],
            "max-leaves": [256],
            "n-estimators": [1000],
            "objective": ["binary:logistic"],
            "tree-method": ["hist"]
        }
    ]
}
