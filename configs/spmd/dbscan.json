            ]
        },
        "synthetic data": {
            "data": [
                { "source": "make_blobs", "generation_kwargs": { "n_samples": 400000,   "n_features": 24, "centers": 10 }, "algorithm": { "estimator_params": { "eps": 5, "min_samples": 10 } } },
                { "source": "make_blobs", "generation_kwargs": { "n_samples": 100000,   "n_features": 50, "centers": 20 }, "algorithm": { "estimator_params": { "eps": 5, "min_samples": [ 20, 30 ] } } },
