{
    "PARAMETERS_SETS": {
        "implementations": {
            "algorithm": [
                { "library": "sklearnex", "device": "distributed", "num_procs": 2, "procs_per_node": 2 }
            ]
        },
        "kmeans parameters": {
            "algorithm": {
                "estimator": "KMeans",
                "estimator_params": {
                    "algorithm": "lloyd"
                }
            },
			"data": {
				"dtype": ["float32", "float64"]
			}
        },
        "real datasets": {
            "data": [
                { "dataset": "higgs",   "dataset_kwargs": { "train_file": "higgs_50t_test.csv" },    "algorithm": { "estimator_params": [ { "n_clusters": 10, "max_iter": 100 }, { "n_clusters": 100, "max_iter": 10 }, { "n_clusters": 250, "max_iter": 10 } ] } },
                { "dataset": "epsilon", "dataset_kwargs": { "train_file": "epsilon_50k_train.csv" }, "algorithm": { "estimator_params": { "n_clusters": [512, 1024, 2048], "max_iter": 10 } } },
                { "dataset": "hepmass", "dataset_kwargs": { "train_file": "hepmass_1m_train.csv" },  "algorithm": { "estimator_params": [ { "n_clusters": 10, "max_iter": 100 }, { "n_clusters": 100, "max_iter": 10 }, { "n_clusters": 250, "max_iter": 10 } ] } },
                { "dataset": "susy",    "dataset_kwargs": { "train_file": "susy_train.csv" },        "algorithm": { "estimator_params": { "n_clusters": 10, "max_iter": 100 } } }
            ]
		},
		"synthetic data": {
			"data": [
				{ "source": "make_blobs", "generation_kwargs": { "n_samples": 400000,   "n_features": 24, "centers": 10 }, "algorithm": { "n_clusters": 10, "max_iter": 10 } },
				{ "source": "make_blobs", "generation_kwargs": { "n_samples": 100000,   "n_features": 50, "centers": 20 }, "algorithm": { "n_clusters": [20, 30], "max_iter": 10 } },
				{ "source": "make_blobs", "generation_kwargs": { "n_samples": 80000000, "n_features": 28, "centers": 30 }, "algorithm": { "n_clusters": 30, "max_iter": 10 } },
				{ "source": "make_blobs", "generation_kwargs": { "n_samples": 1000000,  "n_features": 40, "centers": 10 }, "algorithm": { "n_clusters": 10, "max_iter": 10 } },
				{ "source": "make_blobs", "generation_kwargs": { "n_samples": 2000000,  "n_features": 20, "centers": 30 }, "algorithm": { "n_clusters": 30, "max_iter": 10 } }
			]
		}
	},
    "TEMPLATES": {
        "KMeans Real Datasets": {
            "INCLUDE": ["implementations", "kmeans parameters", "real datasets"]
