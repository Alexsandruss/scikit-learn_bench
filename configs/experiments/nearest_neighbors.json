{
    "PARAMETERS_SETS": {
        "implementations": [
            {
                "algorithm": {
                    "library": ["sklearn", "sklearnex"],
                    "device": "cpu",
                    "estimator_params": { "algorithm": "brute" }
                }
            },
            {
                "algorithm": {
                    "library": "sklbench.emulators.raft",
                    "device": "gpu",
                    "estimator_params": { "algorithm": ["brute", "ivf_flat", "ivf_pq", "cagra"] }
                },
                "data": {
                    "format": "cupy",
                    "order": "C"
                }
            },
            {
                "algorithm": {
                    "library": "sklbench.emulators.faiss",
                    "device": ["cpu", "gpu"],
                    "estimator_params": { "algorithm": ["brute", "ivf_flat", "ivf_pq"] }
                }
            }
        ],
        "common parameters": {
            "algorithm": {
                "estimator": "NearestNeighbors",
                "estimator_params": {
                    "metric": "euclidean",
                    "n_neighbors": [8, 32, 128]
                },
                "estimator_methods": {
                    "training": "fit",
                    "inference": "kneighbors"
                }
            }
        },
        "synth data": {
            "data": {
                "dtype": "float32",
                "source": "make_classification",
                "generation_kwargs": {
                    "n_samples": [25000, 50000, 100000],
                    "n_features": [16, 64, 256, 1024, 4096],
                    "n_classes": 5,
                    "n_informative": "[SPECIAL_VALUE]0.75",
                    "n_redundant": 0,
                    "random_state": 42
                },
                "split_kwargs": {
                    "train_size": 0.2,
                    "test_size": 0.8,
                    "random_state": 42
                }
            }
        }
    },
    "TEMPLATES": {
        "nearest neighbors": {
            "SETS": ["implementations", "common parameters", "synth data"]
        }
    }
}
